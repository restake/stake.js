{
  "name": "@restake/staking-sdk",
  "version": "0.0.1",
  "description": "Restake staking SDK",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./protocol/avalanche": {
      "import": "./dist/esm/protocol/avalanche/index.js",
      "default": "./dist/esm/protocol/avalanche/index.js",
      "require": "./dist/cjs/protocol/avalanche/index.js",
      "types": "./dist/esm/protocol/avalanche/index.d.ts"
    },
    "./protocol/ethereum": {
      "import": "./dist/esm/protocol/ethereum/index.js",
      "default": "./dist/esm/protocol/ethereum/index.js",
      "require": "./dist/cjs/protocol/ethereum/index.js",
      "types": "./dist/esm/protocol/ethereum/index.d.ts"
    },
    "./protocol/near": {
      "import": "./dist/esm/protocol/near/index.js",
      "default": "./dist/esm/protocol/near/index.js",
      "require": "./dist/cjs/protocol/near/index.js",
      "types": "./dist/esm/protocol/near/index.d.ts"
    },
    "./wallet/filesystem": {
      "import": "./dist/esm/wallet/filesystem/index.js",
      "default": "./dist/esm/wallet/filesystem/index.js",
      "require": "./dist/cjs/wallet/filesystem/index.js",
      "types": "./dist/esm/wallet/filesystem/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "build:cjs_old": "tsc -p tsconfig.cjs.json && esbuild --tsconfig=tsconfig.cjs.json --bundle --sourcemap --platform=node    --outdir=dist/cjs src/index.ts",
    "build:esm_old": "tsc -p tsconfig.esm.json && esbuild --tsconfig=tsconfig.esm.json --bundle --sourcemap --platform=browser --outdir=dist/esm src/index.ts",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "author": "",
  "license": "Proprietary",
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@types/bn.js": "5.1.1",
    "@types/elliptic": "6.4.14",
    "@types/node": "18.14.2",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "jest": "29.5.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.0.3"
  },
  "dependencies": {
    "@noble/curves": "0.7.3",
    "@noble/hashes": "1.3.0",
    "avalanche": "3.16.0",
    "bech32": "2.0.0",
    "bn.js": "5.2.1",
    "bs58": "5.0.0",
    "ethers": "^6.7.1",
    "near-api-js": "1.1.0"
  }
}

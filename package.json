{
  "name": "@restake/staking-sdk",
  "version": "0.0.1",
  "description": "Restake staking SDK",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./core": {
      "import": "./dist/esm/core/index.js",
      "default": "./dist/esm/core/index.js",
      "require": "./dist/cjs/core/index.js",
      "types": "./dist/esm/core/index.d.ts"
    },
    "./core/protocol/avalanche": {
      "import": "./dist/esm/core/protocol/avalanche/index.js",
      "default": "./dist/esm/core/protocol/avalanche/index.js",
      "require": "./dist/cjs/core/protocol/avalanche/index.js",
      "types": "./dist/esm/core/protocol/avalanche/index.d.ts"
    },
    "./core/protocol/ethereum": {
      "import": "./dist/esm/core/protocol/ethereum/index.js",
      "default": "./dist/esm/core/protocol/ethereum/index.js",
      "require": "./dist/cjs/core/protocol/ethereum/index.js",
      "types": "./dist/esm/core/protocol/ethereum/index.d.ts"
    },
    "./core/protocol/near": {
      "import": "./dist/esm/core/protocol/near/index.js",
      "default": "./dist/esm/core/protocol/near/index.js",
      "require": "./dist/cjs/core/protocol/near/index.js",
      "types": "./dist/esm/core/protocol/near/index.d.ts"
    },
    "./core/signer": {
      "import": "./dist/esm/core/signer/index.js",
      "default": "./dist/esm/core/signer/index.js",
      "require": "./dist/cjs/core/signer/index.js",
      "types": "./dist/esm/core/signer/index.d.ts"
    },
    "./core/signer/provider": {
      "import": "./dist/esm/core/signer/provider/index.js",
      "default": "./dist/esm/core/signer/provider/index.js",
      "require": "./dist/cjs/core/signer/provider/index.js",
      "types": "./dist/esm/core/signer/provider/index.d.ts"
    },
    "./core/signer/provider/codec": {
      "import": "./dist/esm/core/signer/provider/codec/index.js",
      "default": "./dist/esm/core/signer/provider/codec/index.js",
      "require": "./dist/cjs/core/signer/provider/codec/index.js",
      "types": "./dist/esm/core/signer/provider/codec/index.d.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:cjs_old": "tsc -p tsconfig.cjs.json && esbuild --tsconfig=tsconfig.cjs.json --bundle --sourcemap --platform=node    --outdir=dist/cjs src/index.ts",
    "build:esm_old": "tsc -p tsconfig.esm.json && esbuild --tsconfig=tsconfig.esm.json --bundle --sourcemap --platform=browser --outdir=dist/esm src/index.ts",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "author": "",
  "license": "Proprietary",
  "devDependencies": {
    "@types/bn.js": "5.1.1",
    "@types/elliptic": "6.4.14",
    "@types/node": "18.14.2",
    "@types/ws": "8.5.4",
    "typescript": "5.0.0-beta"
  },
  "dependencies": {
    "@ethereumjs/common": "3.1.1",
    "@ethereumjs/tx": "4.1.1",
    "@noble/curves": "0.7.3",
    "avalanche": "3.16.0",
    "bech32": "2.0.0",
    "bn.js": "5.2.1",
    "bs58": "5.0.0",
    "near-api-js": "1.1.0"
  }
}
